#include "mbed.h"

DigitalIn myButtons[] = {(p11),(p12)};
BusOut myLEDs (p5,p6,p7,p8,p9,p10);

int main(){
    myLEDs=0;
    while(1){
        myLEDs = myLEDs|0x0c;
        wait_ms(100);
        myLEDs = myLEDs|0x0c;
        wait_ms(100);
        myLEDs = myLEDs|0x0c;
        wait_ms(100);
        myLEDs = myLEDs|0x14;
        wait_ms(100);
        myLEDs = myLEDs|0x24;
        wait_ms(100);
        myLEDs = myLEDs|0x26;
        wait_ms(100);
        myLEDs = myLEDs|0x21;
        wait_ms(100);
        myLEDs = myLEDs|0x21;
        wait_ms(100);
        myLEDs = myLEDs|0x21;
        wait_ms(100);
        myLEDs = myLEDs|0x22;
        wait_ms(100);
        myLEDs = myLEDs|0x24;
        wait_ms(100);
        myLEDs = myLEDs|0x34;
        wait_ms(100);
        myLEDs = myLEDs|0x0c;
        wait_ms(100);
    }
}
