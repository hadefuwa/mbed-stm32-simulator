#include "mbed.h"

DigitalIn myButtons[] = {(p11),(p12)};
BusOut myLEDs (p5,p6,p7,p8,p9,p10);

int main(){
    myLEDs=0;
    while(1){
        myLEDs = myLEDs|0x0c;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x0c;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x0c;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x0a;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x09;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x19;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x21;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x21;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x21;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x11;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x09;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x0b;
        wait_ms(1000);
        myLEDs=0;
        myLEDs = myLEDs|0x06;
        wait_ms(1000);
        myLEDs=0;
    }
}
